<#import "parts/common.ftl" as c><#import "parts/pager.ftl" as p><@c.page>    <h3>${userChannel.username}</h3>    <br/><h5>${type}</h5>    <hr style="background: purple;"/>    <ul class="list-group">        <#list page.content as sub>            <li class="list-group-item">                <a href="/user-messages/${sub.id}">${sub.getUsername()}</a>            </li>        </#list>    </ul>    <@p.pager url page/></@c.page>