<#import "parts/common.ftl" as c><#import "parts/messageEdit.ftl" as e><#import "parts/pager.ftl" as p><@c.page>    <h2>Search by "${searchText}"</h2>    <table class="table table-borderless mt-3">        <thread>            <tr class="td-title">                <th scope="col">Name</th>                <th scope="col">Subscribers</th>                <th scope="col">Subscriptions</th>                <th scope="col">Posts</th>            </tr>        </thread>        <tbody>        <#list page.content as user>            <tr scope="row">                <td><a href = "/user-messages/${user.id}">${user.username}</a> </td>                <td>${user.countSubscribers}</td>                <td>${user.countSubscriptions}</td>                <td>${user.countMessages}</td>            </tr>        </#list>        </tbody>    </table><#--    <@p.pager url page/>--></@c.page>