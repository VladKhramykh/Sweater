<#import "parts/common.ftl" as c><#import "parts/pager.ftl" as p><@c.page>    <h3>${userChannel.username}</h3>    <div>${type}</div>    <ul class="list-group">        <@p.pager url page/>        <#list page.content as sub>            <li class="list-group-item">                <a href="/user-messages/${sub.id}">${sub.getUsername()}</a>            </li>        </#list>    </ul></@c.page>