<#import "parts/common.ftl" as c><#import "parts/messageEdit.ftl" as e /><@c.page>    <div class="row">        <div class="col mt-3">            <h3>${userChannel.username}</h3>        </div>        <div class="col">            <#if !isCurrentUser>                <#if isSubscriber>                    <button class="btn link-a-l my-3" onclick="location.href='/user/unsubscribe/${userChannel.id}'">Unsubscribe</button>                <#else >                    <button class="btn link-a-fill-l my-3" onclick="location.href='/user/subscribe/${userChannel.id}'">Subscribe</button>                </#if>            </#if>        </div>    </div>    <div class="container mt-4">        <div class="row">            <div class="col">                <div class="card">                    <div class="card-body">                        <div class="card-title">Subscriptions</div>                        <h3 class="card-text">                            <a class="number" href="/user/subscriptions/${userChannel.id}/list">${subscriptionsCount}</a>                        </h3>                    </div>                </div>            </div>            <div class="col">                <div class="card">                    <div class="card-body">                        <div class="card-title">Subscribers</div>                        <h3 class="card-text">                            <a class="number" href="/user/subscribers/${userChannel.id}/list">${subscribersCount}</a>                        </h3>                    </div>                </div>            </div>        </div>    </div>    <div class="container my-3">        <#if isCurrentUser>            <@e.addNewMessage false />        </#if>    </div>    <#include "parts/messageList.ftl" /></@c.page>