<#import "parts/common.ftl" as c><@c.page><h2>List of users</h2><table class="table table-borderless mt-3">    <thread>        <tr>            <th scope="col">Name</th>            <th scope="col">Role</th>            <th scope="col"></th>        </tr>    </thread>    <tbody>    <#list users as user>        <tr scope="row">            <td>${user.username}</td>            <td><#list user.roles as role>${role}<#sep>, </#list></td>            <td><button type="button" class="btn btn-light" onclick="location.href = '/user/${user.id}'">Edit</button> </td>        </tr>    </#list>    </tbody></table><#list users as user></#list></@c.page>